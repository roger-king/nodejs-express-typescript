{
  "name": "nodejs-express-typescript-seed",
  "version": "1.0.0",
  "description": "NodeJS and express (typed) project seed",
  "main": "index.js",
  "scripts": {
    "start:dev": "DEBUG=* nodemon --exec ./node_modules/.bin/ts-node -- ./src/server.ts",
    "start": "DEBUG=* node dist/",
    "prod:run": "export NODE_ENV=production pm2 start dist",
    "prod:compile": "gulp",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test": "./node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "www.github.com"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "roger king",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.tests.(ts|js)$"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.3",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/helmet": "0.0.34",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-tsc": "^1.3.2",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "ts-jest": "^22.4.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "morgan": "^1.9.0"
  }
}
