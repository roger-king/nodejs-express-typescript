{
  "name": "nodejs-express-typescript-seed",
  "version": "1.0.0",
  "description": "NodeJS and express (typed) project seed",
  "main": "index.js",
  "scripts": {
    "dev": "DEBUG=* nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "start": "DEBUG=* node dist/",
    "prod:run": "export NODE_ENV=production pm2 start dist",
    "prod:compile": "gulp",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test": "./node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "www.github.com"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "roger king",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.tests.(ts|js)$"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.3",
    "@types/chai": "^3.5.1",
    "@types/compression": "0.0.33",
    "@types/config": "0.0.32",
    "@types/cors": "^2.8.1",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.35",
    "@types/helmet": "0.0.34",
    "@types/mocha": "^2.2.41",
    "@types/mongoose": "^4.7.11",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.13",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-tsc": "^1.3.2",
    "jest": "^19.0.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "ts-jest": "^19.0.10",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "config": "^1.25.1",
    "cors": "^2.8.3",
    "debug": "^2.6.4",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "mongoose": "^4.9.5",
    "morgan": "^1.8.1"
  }
}
